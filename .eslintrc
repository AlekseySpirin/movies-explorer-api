{
  "extends": [
    "airbnb-base",
    "eslint:recommended",
    "plugin:prettier/recommended",
    "eslint-config-prettier"
  ],
  "plugins": [
    "prettier"
  ],
  "rules": {
    "class-methods-use-this": "off",
    "prettier/prettier": [
      "error",
      {
        "singleQuote": false,
        "trailingComma": "all"
      }
    ],
    "quotes": [
      "error",
      "double"
    ],
    "no-console": "off",
    "import/extensions": [
      "error",
      "never"
    ],
    "no-underscore-dangle": [
      "error",
      {
        "allow": [
          "_id"
        ]
      }
    ]
  },
  "env": {
    "browser": true,
    "es2022": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 2022,
    "sourceType": "module",
    "jsx": true
  }
}
